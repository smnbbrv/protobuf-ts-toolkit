{
  "name": "@protobuf-ts-toolkit/grpc-web-hybrid",
  "version": "1.0.0",
  "description": "Hybrid gRPC-web transport: HTTP for unary/server-streaming, WebSocket for client-streaming/duplex",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smnbbrv/protobuf-ts-toolkit",
    "directory": "packages/grpc-web-hybrid"
  },
  "keywords": [
    "grpc",
    "grpc-web",
    "protobuf",
    "protobuf-ts",
    "transport",
    "websocket",
    "streaming",
    "typescript"
  ],
  "author": "smnbbrv",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.15.0"
  },
  "peerDependencies": {
    "@protobuf-ts/runtime": "^2.0.0",
    "@protobuf-ts/runtime-rpc": "^2.0.0",
    "@protobuf-ts/grpcweb-transport": "^2.0.0"
  },
  "devDependencies": {
    "@protobuf-ts/runtime": "^2.9.0",
    "@protobuf-ts/runtime-rpc": "^2.9.0",
    "@protobuf-ts/grpcweb-transport": "^2.9.0",
    "typescript": "^5.7.0",
    "tsup": "^8.0.0"
  }
}
