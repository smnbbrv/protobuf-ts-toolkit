{
  "name": "@protobuf-ts-toolkit/metadata",
  "version": "1.0.0",
  "description": "Metadata interceptor for gRPC: supports static, sync, and async metadata with streaming support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smnbbrv/protobuf-ts-toolkit",
    "directory": "packages/metadata"
  },
  "keywords": [
    "grpc",
    "protobuf",
    "protobuf-ts",
    "interceptor",
    "metadata",
    "headers",
    "authorization",
    "typescript"
  ],
  "author": "smnbbrv",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@protobuf-ts/runtime-rpc": "^2.0.0"
  },
  "devDependencies": {
    "@protobuf-ts/runtime-rpc": "^2.9.0",
    "typescript": "^5.7.0",
    "tsup": "^8.0.0"
  }
}
