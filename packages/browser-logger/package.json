{
  "name": "@protobuf-ts-toolkit/browser-logger",
  "version": "1.0.2",
  "description": "Browser console logging interceptor for gRPC calls with styled output",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smnbbrv/protobuf-ts-toolkit",
    "directory": "packages/browser-logger"
  },
  "keywords": [
    "grpc",
    "protobuf",
    "protobuf-ts",
    "logging",
    "browser",
    "console",
    "debug",
    "typescript"
  ],
  "author": "smnbbrv",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@protobuf-ts/runtime": "^2.0.0",
    "@protobuf-ts/runtime-rpc": "^2.0.0"
  },
  "devDependencies": {
    "@protobuf-ts/runtime": "^2.9.0",
    "@protobuf-ts/runtime-rpc": "^2.9.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "tsup": "^8.0.0"
  }
}
